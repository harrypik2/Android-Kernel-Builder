[
  {
    "kernelSource": {
      "name": "Raphael-KernelSU-Proton-release",
      "repo": "https://github.com/harrypik2/SnowKsuKernel",
      "branch": "T",
      "device": "raphael",
      "defconfig": "raphael_defconfig"
    },
    "withKernelSU": false,
    "toolchains": [
      {
        "repo": "https://gitlab.com/ElectroPerf/atom-x-clang",
        "branch": "master",
        "name": "atom-x-clang",
        "binPath": ["bin"]
      }
    ],
    "ccache": true,
    "params": {
      "ARCH": "arm64",
      "CC": "clang",
      "externalCommand": {
        "CROSS_COMPILE": "aarch64-linux-gnu-",
        "CROSS_COMPILE_ARM32": "arm-linux-gnueabi-",
        "LD": "ld.lld",
        "AR": "llvm-ar",
        "NM": "llvm-nm",
        "OBJCOPY": "llvm-objcopy",
        "OBJDUMP": "llvm-objdump",
        "READELF": "llvm-readelf",
        "OBJSIZE": "llvm-size",
        "STRIP": "llvm-strip",
        "LDGOLD": "llvm-ld.gold",
        "LLVM_AR": "llvm-ar",
        "LLVM_DIS": "llvm-dis"
      }
    },
    "AnyKernel3": {
      "use": true,
      "release": false,
      "repo": "https://github.com/harrypik2/AnyKernel3",
      "branch": "master"
    }
  }
]
